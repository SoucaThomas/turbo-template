{
  "name": "@turbo-template/api-client",
  "version": "1.0.0",
  "description": "Auto-generated API client for TanStack Query",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate-types": "curl -s http://localhost:9095/api-json > openapi-spec.json && openapi-typescript openapi-spec.json -o src/types.ts",
    "clean": "rm -rf dist"
  },
  "keywords": ["api", "client", "tanstack", "query", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "openapi-typescript": "^6.7.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
